autocomplete()
  width: 100%
  padding: 0 10px
  position: fixed
  z-index: 998

radio-group()
  margin-top: 15px
  display: flex
  justify-content: space-between
  
tips()
  color: #646464
  background-color: #f5f5f5
  line-height: 24px
  padding: 2px
  margin-top: 10px

ellipsis()
  overflow: hidden
  white-space: nowrap
  text-overflow: ellipsis